       #Page-6 Product Screen



import React, { Component } from 'react';
import { Text, View, ScrollView, StyleSheet,
        Image, TouchableOpacity ,Dimensions} from 'react-native';
import {
            heightPercentageToDP as hp,
            widthPercentageToDP as wp,
           } from 'react-native-responsive-screen';


 const images = [require('../images/pbag1.jpg'),
                 require('../images/pbag2.jpg'),
                 require('../images/pbag3.jpg')];
                
let device_width=Dimensions.get("window").width;
let screenDecide = {

    height: Dimensions.get("window").height,
    width: Dimensions.get("window").width,  
};
 class product extends Component {
    scrollRef=React.createRef()
    constructor(props){
        super(props);
        this.state = {
          selectedIndex:0,
        //   isLandscape:false
        }
    }

    setSelectedIndex= event => {                                   //white button work with open for manual slide
        const viewSize= event.nativeEvent.layoutMeasurement.width;
        const contentOffset= event.nativeEvent.contentOffset.x;
        const selectedIndex=Math.floor(contentOffset/viewSize);
        this.setState({selectedIndex})
    }                                                               //white button work with open for manual slide

    // componentDidMount(){                            //open for auto slide
      
    //     setInterval(()=>{
    //       this.setState((prev) => ({selectedIndex:
    //         prev.selectedIndex===images.length-1 
    //         ? 0
    //         : prev.selectedIndex+1}),
    //        () => {
    //            this.scrollRef.current.scrollTo({
    //                animated:true,
    //                y:0,
    //                x:device_width*this.state.selectedIndex
    //            })
    //        }
    //       )
    //     },8000)
    // }                                                  //open for auto slide


    render() {
        const {selectedIndex}=this.state
        return (
          
            <View style={styles.container}>
             
              <ScrollView>  
                <View>
                 <View style={[styles.autoScrollTop,]}>   
                        
                   <ScrollView horizontal pagingEnabled 
                       onMomentumScrollEnd={this.setSelectedIndex} //white button work with open for manual slide

                       // {width:screenDecide.width}
                       showsHorizontalScrollIndicator={false}
                       ref={this.scrollRef}
                       >
                          {images.map((item) =>
                             { return (
                                <Image style={[styles.autoScrollTop,{resizeMode:'stretch'}]}
                                source={item}
                                /> )
                            }
                            )}
                       </ScrollView> 
                       <View style={styles.circleView}>
                           {
                               images.map((item,i) =>
                               { return(
                                  <View key={i} style={[styles.whiteCircle,{opacity: i===selectedIndex?1:0.5}]}/>
                               )}
                               )
                           }
                           
                       </View>

                       <View style={{ flex: 3 }}> 
                        <TouchableOpacity  onPress={() => {this.props.navigation.navigate('women')}}>
                         <Image source={require('../images/left.png')}  
                          style={{width: 25, height:25,bottom:280,left:5,tintColor:"white"}} /> 
                        </TouchableOpacity>                  
